{
	"name": "rentx",
	"version": "1.0.0",
	"main": "src/shared/infra/http/server.ts",
	"license": "MIT",
	"scripts": {
		"dev": "tsnd -r tsconfig-paths/register -r dotenv/config --inspect --transpile-only --ignore-watch node_modules --respawn .",
		"typeorm": "tsnd -r tsconfig-paths/register ./node_modules/typeorm/cli",
		"migration:create": "yarn typeorm migration:create -n",
		"migration:up": "yarn typeorm migration:run",
		"migration:down": "yarn typeorm migration:revert",
		"seed:admin": "tsnd src/shared/infra/typeorm/seed/admin.ts",
		"lint": "eslint ./src/**/**",
		"lintFix": "eslint ./src/**/** --fix",
		"format": "prettier --write \"**/*.{js,json,md,ts,hbs}\"",
		"test": "NODE_ENV=test jest --runInBand --detectOpenHandles --verbose --setupFiles dotenv/config"
	},
	"dependencies": {
		"aws-sdk": "^2.1155.0",
		"bcryptjs": "^2.4.3",
		"class-transformer": "^0.5.1",
		"csv-parse": "^5.0.4",
		"dayjs": "^1.11.3",
		"dotenv": "^16.0.1",
		"express": "^4.18.1",
		"express-async-errors": "^3.1.1",
		"handlebars": "^4.7.7",
		"jsonwebtoken": "^8.5.1",
		"mime": "^3.0.0",
		"multer": "^1.4.4",
		"nodemailer": "^6.7.5",
		"pg": "^8.7.3",
		"reflect-metadata": "^0.1.13",
		"swagger-ui-express": "^4.4.0",
		"tsyringe": "^4.6.0",
		"typeorm": "0.2.45",
		"uuid": "^8.3.2"
	},
	"devDependencies": {
		"@types/aws-sdk": "^2.7.0",
		"@types/bcryptjs": "^2.4.2",
		"@types/express": "^4.17.13",
		"@types/handlebars": "^4.1.0",
		"@types/jest": "^27.5.1",
		"@types/jsonwebtoken": "^8.5.8",
		"@types/mime": "^2.0.3",
		"@types/multer": "^1.4.7",
		"@types/nodemailer": "^6.4.4",
		"@types/supertest": "^2.0.12",
		"@types/swagger-ui-express": "^4.1.3",
		"@types/uuid": "^8.3.4",
		"@typescript-eslint/eslint-plugin": "^5.25.0",
		"@typescript-eslint/parser": "^5.25.0",
		"eslint": "^8.15.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-import-helpers": "^1.2.1",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-unused-imports": "^2.0.0",
		"jest": "^28.1.0",
		"prettier": "^2.6.2",
		"supertest": "^6.2.3",
		"ts-jest": "^28.0.3",
		"ts-node-dev": "^1.1.8",
		"tsconfig-paths": "^4.0.0",
		"typescript": "^4.6.4",
		"typescript-eslint": "^0.0.1-alpha.0"
	}
}
